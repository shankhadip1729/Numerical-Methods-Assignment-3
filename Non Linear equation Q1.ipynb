   "source": [
    "from scipy.optimize import bisect\n",
    "import numpy as np\n",
    "\n",
    "\n",
    "def f(x):\n",
    "    return np.sin(np.cos(np.exp(x)))\n",
    "\n",
    "a, b = -1, 1\n",
    "\n",
    "root = bisect(f, a, b)\n",
    "\n",
    "value_at_root = f(root)\n",
    "\n",
    "print(f\"Root: {root}\")\n",
    "print(f\"Value at Root: {value_at_root}\")"
   ]
  }
 ],
